{
  "$schema": "https://json.schemastore.org/prettierrc",
  "tabWidth": 4,
  "useTabs": false,
  "semi": false,
  "printWidth": 150,
  "arrowParens": "always",
  "endOfLine": "lf",
  "singleQuote": false,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2
      }
    }
  ],
  "bracketSpacing": true
}